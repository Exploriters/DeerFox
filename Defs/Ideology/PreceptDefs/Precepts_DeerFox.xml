    <impact>High</impact>
    <displayOrderInIssue>0</displayOrderInIssue>
    <displayOrderInImpact>1000</displayOrderInImpact>
    <associatedMemes>
      <li>DeerFoxMeme</li>
    </associatedMemes>
    <requiredMemes>
      <li>DeerFoxMeme</li>
    </requiredMemes>
    <comps>
      <li Class="PreceptComp_SituationalThought">
        <thought>Slavery_Honorable_SlavesInColony_DeerFox</thought>
        <description>殖民地中的每个奴隶</description>
      </li>
      <li Class="PreceptComp_SelfTookMemoryThought">
        <eventDef>SoldSlave</eventDef>
        <thought>SoldSlave_Honorable_DeerFox</thought>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_SelfTookMemoryThought">
        <eventDef>EnslavedPrisonerNotPreviouslyEnslaved</eventDef>
        <thought>EnslavedPrisoner_Honorable_DeerFox</thought>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>SoldSlave</eventDef>
        <thought>SoldSlave_Know_Honorable_DeerFox</thought>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>EnslavedPrisonerNotPreviouslyEnslaved</eventDef>
        <thought>EnslavedPrisoner_Know_Honorable_DeerFox</thought>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>SoldSlave</eventDef>
        <thought>SoldSlave_Know_Honorable_Mood_DeerFox</thought>
        <description>有人出售奴隶</description>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>EnslavedPrisonerNotPreviouslyEnslaved</eventDef>
        <thought>EnslavedPrisoner_Know_Honorable_Mood_DeerFox</thought>
        <description>有人奴役了囚犯</description>
        <onlyForNonSlaves>true</onlyForNonSlaves>
      </li>
      <li Class="PreceptComp_DevelopmentPoints">
        <eventDef>EnslavedPrisonerNotPreviouslyEnslaved</eventDef>
        <eventLabel>奴役了囚犯</eventLabel>
        <points>1</points>
      </li>
    </comps>
    <approvesOfSlavery>true</approvesOfSlavery>
  </PreceptDef>
